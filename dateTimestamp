
SELECT CONCAT(COLUMN1, COLUMN2) AS ConcatenateColumn

SELECT LOWER(COLUMN) AS LowerColumn

SELECT UPPER(COLUMN) AS UpperColumn

SELECT TRIM(COLUMN) AS TrimColumn

SELECT LENGTH(COLUMN) AS LengthOfColumn

SELECT SUBSTRING(COLUMN, START, LENGTH) AS SubstringColumn

SELECT ROUND(COLUMN, DECIMAL_PLACES) AS RoundedColumn

SELECT DATE_ADD(COLUMN, INTERVAL EXPRESSION UNIT) AS NewDate

SELECT DATE_SUB(COLUMN, INTERVAL EXPRESSION UNIT) AS NewDate

SELECT FORMAT(COLUMN, FORMAT_STRING) AS FormatValue

SELECT DATE_ADD(DATE_ADD(NOW(), INTERVAL 19 DAY), INTERVAL 6 MONTH) AS BIRTHDAY_DATE;



